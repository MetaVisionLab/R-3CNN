name: "resnet18_hinge_train"
layer {
  name: "data"
  type: "ImageData"
  top: "data"
  top: "label_siamese"
  image_data_param {
    source: "../../data/1/train.txt"
    batch_size: 4
    new_height: 256
    new_width: 256
    root_folder: "../../data/faces/"
    shuffle: false
    reference: true
  }
  transform_param {
    crop_size: 224
    mean_file: "../../data/1/256_single_mean.binaryproto"
    mirror: true
    scale: 0.00392156862745
  }
}

layer {
  name: "p_data"
  type: "ImageData"
  top: "p_data"
  top: "p_label_siamese"
  image_data_param {
    source: "../../data/1/train_p.txt"
    batch_size: 4
    new_height: 256
    new_width: 256
    root_folder: "../../data/faces/"
    shuffle: false
    reference: true
  }
  transform_param {
    crop_size: 224
    mean_file: "../../data/1/256_single_p_mean.binaryproto"
    scale: 0.00392156862745
    mirror: true
  }
}

layer {
  name: "single_label"
  type: "ImageData"
  top: "single_label_none"
  top: "single_label"
  image_data_param {
    source: "../../data/1/single.txt"
    batch_size: 4
    reference: false
    shuffle: false
  }
}

layer {
  name: "p_single_label"
  type: "ImageData"
  top: "p_single_label_none"
  top: "p_single_label"
