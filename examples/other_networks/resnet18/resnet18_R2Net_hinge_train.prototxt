    source: "../../data/1/single_p.txt"
    batch_size: 4
    reference: false
    shuffle: false
  }
}
layer {
  name: "silence"
  type: "Silence"
  bottom: "p_label_siamese"
  bottom: "single_label_none"
  bottom: "p_single_label_none"
}


layer {
    bottom: "data"
    top: "conv1"
    name: "conv1"
    type: "Convolution"
    param {
        name: "conv1_w"
        lr_mult: 1
        decay_mult: 1
    }
    convolution_param {
        num_output: 64
        kernel_size: 7
        pad: 3
        stride: 2
        weight_filler {
            type: "msra"
        }
        bias_term: false

    }
}

layer {
    bottom: "conv1"
    top: "conv1"
    name: "bn_conv1"
    type: "BatchNorm"
